{
  "kind": "collectionType",
  "collectionName": "games",
  "info": {
    "singularName": "game",
    "pluralName": "games",
    "displayName": "game",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "launched_date": {
      "type": "date",
      "required": true
    },
    "link": {
      "type": "string"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "editor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::editor.editor",
      "inversedBy": "games"
    },
    "genres": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::genre.genre",
      "inversedBy": "games"
    },
    "platforms": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::platform.platform",
      "inversedBy": "games"
    },
    "developer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::developer.developer",
      "inversedBy": "games"
    },
    "vidLink": {
      "type": "string",
      "required": true
    },
    "sold": {
      "type": "integer",
      "default": 0,
      "required": false
    },
    "commande": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::commande.commande",
      "inversedBy": "games"
    }
  }
}
